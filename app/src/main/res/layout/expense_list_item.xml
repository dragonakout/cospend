<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:orientation="vertical"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">


    <TextView
            android:text="TextView"
            android:layout_width="124dp"
            android:layout_height="26dp"
            android:id="@+id/expense_list_date" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.246" android:textSize="18sp" android:textColor="#616161"
            app:layout_constraintTop_toBottomOf="@+id/expense_list_desc" android:layout_marginTop="12dp"
            android:fontFamily="casual"/>
    <TextView
            android:text="Description"
            android:layout_width="249dp"
            android:layout_height="44dp"
            android:textStyle="bold"
            android:id="@+id/expense_list_desc" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.053" android:textSize="22sp" android:textColor="#202020"
            android:fontFamily="casual" android:includeFontPadding="true"/>
    <TextView
            android:text="42$"
            android:layout_width="94dp"
            android:layout_height="46dp"
            android:id="@+id/expense_list_amount" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="52dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
            android:textSize="22sp" android:textColor="#057E05"
            android:gravity="center" android:fontFamily="casual"/>
</androidx.constraintlayout.widget.ConstraintLayout>